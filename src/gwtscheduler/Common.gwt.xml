<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.0//EN" 
 "http://google-web-toolkit.googlecode.com/svn/tags/1.6.0/distro-source/core/src/gwt-module.dtd">

<!-- GWT Module for common source code-->
<module>
   	<inherits name="com.google.gwt.user.User" />
   	<inherits name="com.google.gwt.uibinder.UiBinder" />
   	<inherits name="com.google.gwt.i18n.I18N"/>
   	
	<!-- Joda-Time emulation via goda/time  -->
	<inherits name='org.goda.Goda' />

	<!-- Useful for rond corners -->    
   	<inherits name='org.cobogw.gwt.user.User' />
   	
   	<!--
   	<inherits name='com.google.gwt.user.theme.standard.Standard'/> 
   	 -->
   	<inherits name='com.google.gwt.user.theme.standard.StandardResources'/>
   	<inherits name="com.google.gwt.inject.Inject"/>
	
	<!-- this comes in handy for debugging css 
	<set-property name="CssResource.globalPrefix" value="empty" />
	<set-property name="CssResource.style" value="pretty" />
	<set-property name="ResourceBundle.enableInlining" value="false" />
	-->

   	<!-- aggregates all common source code (shared between client/server) -->
   	<source path="common" />
   	
</module>